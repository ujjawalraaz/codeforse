#include<iostream>
#define ll long long int
using namespace std;
int main()
{
      ll n;
      cin>>n;
      string str;
      cin>>str;
      ll a=0,d=0;
      for(int i=0;i<n;i++)
      {
            if(str[i]=='A')
            a++;
            else
            d++;
      }
      if(a>d)
      cout<<"Anton"<<endl;
      else{
      if(d>a)
      cout<<"Danik"<<endl;
      else
      cout<<"Friendship"<<endl;
      }
      return(0);
}
