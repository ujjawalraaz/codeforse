#include<iostream>
#define ll long long int
using namespace std;
int main()
{
     string str;
     cin>>str;
     char arr[5];
     arr[0]='h';arr[1]='e';arr[2]='l';arr[3]='l';arr[4]='o';
     ll j=0;
     for(int i=0;i<str.size();i++)
     {
           if(str[i]==arr[j])
           j++;
           if(j==5)
           break;
     }
     if(j==5)
     cout<<"YES"<<endl;
     else
     cout<<"NO"<<endl;
     return(0);
}
