#include<iostream>
#define ll long long int
using namespace std;
int main()
{
      ll n;
      cin>>n;
      ll k=0;
      ll arr[]={4,7,44,47,74,77,444,447,474,477,744,747,774,777};
      ll n1=sizeof(arr)/sizeof(arr[0]);
      for(int i=0;i<n1;i++)
      if(n%arr[i]==0)
      {
            k=1;
            break;
      }
      if(k==1)
      cout<<"YES"<<endl;
      else
      cout<<"NO"<<endl;
      return(0);
}
