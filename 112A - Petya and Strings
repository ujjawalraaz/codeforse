#include<iostream>
#include<bits/stdc++.h>
#define ll long long int
using namespace std;
int main()
{
          string str,str1;
          cin>>str>>str1;
          ll n=str.size();
          transform(str.begin(),str.end(),str.begin(),::toupper);
          transform(str1.begin(),str1.end(),str1.begin(),::toupper);
          for(int i=0;i<n;i++)
          {
                    if(str[i]==str1[i])
                    {}
                    else
                    {
                              if(str[i]<str1[i])
                              {
                                        cout<<"-1"<<endl;
                                        return(0);
                              }
                              else
                              {
                                        cout<<1<<endl;
                                        return(0);
                              }
                    }
          }
          cout<<0<<endl;
          return(0);
}
