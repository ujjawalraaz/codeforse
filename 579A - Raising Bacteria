#include<iostream>
#define ll long long int
using namespace std;
int main()
{
      ll n;
      cin>>n;
      ll num=1;
      ll count=0;
      while(num<=n)
      {
            ll m=num & n;
            if(m!=0)
            count++;
            num=num<<1;
      }
      cout<<count<<endl;
      return(0);
}
