#include<iostream>
#define ll long long int
using namespace std;
int main()
{
     ll n;
     cin>>n;
     string str;
     cin>>str;
     ll l=0,r=0;
     for(int i=0;i<n;i++)
     {
               if(str[i]=='L')
               l++;
               else
               r++;
     }
     cout<<l+r+1<<endl;
     return(0);
}
