int main()
{
   fast;
   string str;
   getline(cin,str);
   ll arr[30]={0};
   rep(i,str.size())
   {
      if(str[i]>=97&&str[i]<=122)
      arr[str[i]-97]=1;
 //  ll h=str[i];
  // cout<<h<<" ";
   }
 //  pr1(str);
   ll sum=0;
   rep(i,30)
  // cout<<arr[i]<<" ";
   sum=sum+arr[i];
   pr1(sum);
